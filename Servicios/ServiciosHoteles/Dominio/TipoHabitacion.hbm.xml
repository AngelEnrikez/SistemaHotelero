<?xml version="1.0" encoding="utf-8" ?>
<!-- ELABORADO POR JOSE PASCUAL -->
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ServiciosHoteles"
                   namespace="ServiciosHoteles.Dominio"
                   default-lazy="false">
  <class name="TipoHabitacion" table="t_TipoHabitacion">
    <id name="IdTipoHabitacion" column="idTipoHabitacion">
      <generator class="increment" />
    </id>
    <property name="Descripcion" column="descripcion" />
    <property name="Tarifa" column="tarifa" />
  </class>
</hibernate-mapping>